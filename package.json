{
  "name": "vava",
  "version": "3.5.0",
  "homepage": "https://letsbug.github.io/vava/",
  "keywords": [
    "vue",
    "element-ui",
    "admin",
    "management",
    "admin",
    "template"
  ],
  "description": "A magical vue admin template, based on vue & element-ui & vava-mini",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/letsbug/vava.git"
  },
  "author": "Letsbug <724939933@qq.com> <xman724939995@gmail.com> (https://github.com/letsbug/vava)",
  "license": "MIT",
  "scripts": {
    "svgo": "svgo -f src/assets/icons --config=./src/assets/icons/svgo.yml",
    "svg_imgs": "svgo -f static/svgs --config=./static/svgs/svgo.yml",
    "css-template": "node-sass --output-style expanded src/themes/theme-template.scss static/css/theme-template.css && npm run css-minify",
    "css-minify": "cleancss --level 1 --format breaksWith=lf --output static/css/theme-template.min.css static/css/theme-template.css",
    "dev": "npm run css-template && cross-env BABEL_ENV=development webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "build": "npm run css-template && cross-env NODE_ENV=production env_config=prod node build/build.js",
    "start": "npm run dev",
    "lint": "eslint --ext .js,.vue src",
    "test": "npm run lint"
  },
  "dependencies": {
    "@tinymce/tinymce-vue": "^1.1.2",
    "axios": "^0.18.0",
    "clipboard": "^2.0.4",
